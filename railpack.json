{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "setup": {
      "commands": [
        "apt-get update && apt-get install -y libatomic1"
      ]
    },
    "install": {
      "inputs": [{ "step": "setup" }],
      "commands": [
        "pnpm install --no-frozen-lockfile"
      ]
    }
  }
}
